{
  "manifest_version": 2,
  "short_name": "AletheoWallet",
  "name": "Aletheo Wallet",
  "description": "Addon that allows you to get paid for posting on the internet",
  "version": "0.2.4.3",
  "author": "Sam Porter",
  "permissions": [
    "storage",
    "*://oracle.aletheo.net/*",
    "*://a.4cdn.org/*",
    "*://boards.4channel.org/*",
    "*://boards.4chan.org/*",
    "*://kohlchan.net/*",
    "*://endchan.net/*",
    "*://endchan.gg/*",
    "*://endchan.org/*",
    "*://www.hispachan.org/*",
    "*://www.diochan.com/*",
    "*://indiachan.com/*",
    "*://ptchan.org/*",
    "*://dobrochan.org/*",
    "*://pajeet.top/*",
    "*://sportschan.org/*"
  ],
  "icons": {
    "32": "theme/icons/let32.png",
    "48": "theme/icons/let48.png",
    "64": "theme/icons/let64.png"
  },
  "background": {
    "page": "popup/home.html"
  },
  "content_scripts": [
    {
      "js": [
        "common/browser-polyfill.min.js",
        "content/collectFormData.js"
      ],
      "matches": [
        "*://boards.4channel.org/*",
        "*://boards.4chan.org/*",
        "*://a.4cdn.org/*",
        "*://2ch.hk/*",
        "*://2ch.pm/*",
        "*://kohlchan.net/*",
        "*://endchan.net/*",
        "*://endchan.gg/*",
        "*://endchan.org/*",
        "*://www.hispachan.org/*",
        "*://www.diochan.com/*",
        "*://indiachan.com/*",
        "*://ptchan.org/*",
        "*://dobrochan.org/*",
        "*://pajeet.top/*",
        "*://sportschan.org/*"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "browser_action": {
    "browser_style": false,
    "default_title": "ALETHEO WALLET",
    "default_popup": "popup/popup.html"
  }
}
